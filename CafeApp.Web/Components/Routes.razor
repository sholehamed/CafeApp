@using CafeApp.Shared.Layouts
@using CafeApp.Shared.Pages
<Router AppAssembly="typeof(Program).Assembly" AdditionalAssemblies="new[]{typeof(AdminLayout).Assembly}">
    <Found Context="routeData">
        <RouteView RouteData="routeData" DefaultLayout="typeof(CafeApp.Shared.Layouts.AdminLayout)" />
        <FocusOnNavigate RouteData="routeData" Selector="h1" />
    </Found>
</Router>
